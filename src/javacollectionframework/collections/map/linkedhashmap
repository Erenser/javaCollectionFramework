import java.util.Map;
import java.util.LinkedHashMap

public class Main {
public static void main(String[] args) {
   
    Map<Integer, String> map = new LinkedHashMap<Integer,String>();
     
     map.put(1, "James");
     map.put(3, "Kevin");
     map.put(15, "Adam");
     map.put(1, "James");
     //Aynı listeyi bir daha sıralamaz (Hashing)
     //Key sırasına göre düzenli sıralar (ID Örneği kücükten büyüğe sıralı)
     //LinkedHashMap Map interface' den implement alır
   for(Map.Entry<Integer,String> m : map.entrySet())
   {
       System.out.println(m);
   }
    
    }

}
